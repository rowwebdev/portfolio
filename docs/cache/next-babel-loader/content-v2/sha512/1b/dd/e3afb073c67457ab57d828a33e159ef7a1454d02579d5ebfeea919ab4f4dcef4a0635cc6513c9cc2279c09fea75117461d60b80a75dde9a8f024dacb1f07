{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport { useContext } from \"react\";\nimport Button from \"../Button\";\nimport ThemeContext from \"../../context/theme-context\";\nimport { THEMES } from \"../Theme\";\nimport sectionStyles from \"../../styles/Section.module.scss\";\nimport styles from \"../../styles/Main.module.scss\";\n\nconst Main = () => {\n  const theme = useContext(ThemeContext);\n  const currentTheme = THEMES[theme.themeId];\n\n  const changeTheme = () => {\n    let themeId = +theme.themeId;\n    if (themeId < 2) themeId++; // go to next theme\n    else themeId = 1; // go back to first theme\n\n    theme.switchTheme(themeId);\n  };\n\n  const classNames = [sectionStyles.Section, styles.Main];\n  classNames.push(styles[`Theme${theme.themeId}`]);\n  return /*#__PURE__*/_jsxs(\"section\", {\n    className: classNames.join(\" \"),\n    children: [/*#__PURE__*/_jsxs(\"div\", {\n      className: styles.Container,\n      children: [/*#__PURE__*/_jsxs(\"header\", {\n        children: [/*#__PURE__*/_jsx(\"h2\", {\n          children: \"Rowan Lawrence\"\n        }), /*#__PURE__*/_jsx(\"nav\", {\n          children: /*#__PURE__*/_jsx(\"div\", {\n            onClick: changeTheme,\n            children: /*#__PURE__*/_jsx(\"span\", {\n              className: \"iconify\",\n              \"data-icon\": \"clarity:switch-line\",\n              \"data-inline\": \"false\"\n            })\n          })\n        })]\n      }), /*#__PURE__*/_jsxs(\"div\", {\n        className: styles.Hero,\n        children: [/*#__PURE__*/_jsx(\"h1\", {\n          children: \"Loves to create\"\n        }), /*#__PURE__*/_jsx(\"h2\", {\n          children: \"Eager to learn\"\n        }), /*#__PURE__*/_jsx(\"p\", {\n          children: \"A full stack developer currently searching for the next opportunity to create amazing user experiences, learn from talented people, and contribute to a successful team.\"\n        }), /*#__PURE__*/_jsx(Button, {\n          children: \"Learn More\"\n        })]\n      }), /*#__PURE__*/_jsx(\"div\", {\n        className: styles.CallToAction\n      })]\n    }), /*#__PURE__*/_jsx(\"div\", {\n      className: styles.Overlay\n    }), /*#__PURE__*/_jsx(\"video\", {\n      src: `/assets/${currentTheme.bgVideo}`,\n      muted: true,\n      loop: true,\n      autoPlay: true\n    })]\n  });\n};\n\nexport default Main;","map":null,"metadata":{},"sourceType":"module"}